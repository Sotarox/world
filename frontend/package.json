{
  "name": "world-frontend",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "concurrently \"npm run tailwind:watch\" \"webpack serve\"",
    "build": "npm run tailwind:build && webpack",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "cd .. && husky",
    "tailwind:build": "tailwindcss -i ./public/globals.css -o ./public/output.css",
    "tailwind:watch": "tailwindcss -i ./public/globals.css -o ./public/output.css --watch"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.6",
    "@mui/icons-material": "^7.3.1",
    "@tailwindcss/cli": "^4.1.12",
    "axios": "^1.11.0",
    "flag-icons": "^7.5.0",
    "react": "^19.1.1",
    "react-circle-flags": "^0.0.23",
    "react-dom": "^19.1.1",
    "react-router": "^7.8.0",
    "tailwindcss": "^4.1.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.33.0",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.2.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "3.6.2",
    "style-loader": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
